<div class='container px-5 px-sm-0'>
  <div class="mt-3 mb-5">
    <h2 class="d-inline bg-light p-2 px-3 rounded">投稿一覧(#<%= @tag.name %>)</h2>
  </div>
  <div class='row'>
    <div class='col-md-3'>
     <%= render 'tag', tag_list: @tag_list %>
    </div>
    <div class='col-md-9'>
      <%= render 'public/posts/index', posts: @posts %>
    </div>
  </div>
</div>
